            <height>236</height>
        </rect>
    </property>
    <property name="caption">
        <string>Configure Sound to Light Plugin</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QGroupBox" row="1" column="0">
