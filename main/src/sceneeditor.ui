            <cstring>m_availableScenesComboBox</cstring>
        </property>
        <property name="geometry">
            <rect>
                <x>0</x>
                <y>25</y>
                <width>130</width>
                <height>20</height>
            </rect>
        </property>
        <property name="toolTip" stdset="0">
            <string></string>
        </property>
    </widget>
    <widget class="QButtonGroup">
        <property name="name">
            <cstring>m_saveToGroup</cstring>
        </property>
        <property name="geometry">
            <rect>
                <x>0</x>
                <y>50</y>
                <width>130</width>
                <height>70</height>
            </rect>
        </property>
        <property name="title">
            <string>SRC / DEST</string>
        </property>
