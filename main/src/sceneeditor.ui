            <string>Store</string>
        </property>
        <property name="toolTip" stdset="0">
            <string>Save as selected scene</string>
        </property>
    </widget>
    <widget class="QPushButton">
        <property name="name">
            <cstring>m_newButton</cstring>
        </property>
        <property name="geometry">
            <rect>
                <x>0</x>
                <y>160</y>
                <width>60</width>
